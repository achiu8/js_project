<h3><%= @survey.title %></h3>
<button class="delete-survey" id="<%= @survey.id %>">Delete</button>
<ul>
<% @survey.questions.each do |question| %>
  <li><%= question.question %></li>
  <ul>
  <% question.choices.each do |choice| %>
    <li><%= choice.choice %></li>
  <% end %>
  </ul>
<% end %>
</ul>
